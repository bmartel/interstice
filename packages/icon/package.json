{
	"name": "@interstice/icon",
	"version": "0.0.1",
	"description": "A reusable icon component based on tailwindcss design with zondicons",
	"keywords": [
		"interstice",
		"icon",
		"webcomponent",
		"tailwindcss",
		"zondicons"
	],
	"author": "Brandon Martel <brandonmartel@gmail.com>",
	"homepage": "https://github.com/bmartel/interstice#readme",
	"license": "MIT",
	"scripts": {
		"build": "pika build",
		"prebuild": "npm run generate",
		"generate": "node generate.js",
		"test": "karma start --coverage",
		"test:watch": "karma start --auto-watch=true --single-run=false",
		"test:update-snapshots": "karma start --update-snapshots",
		"test:prune-snapshots": "karma start --prune-snapshots",
		"test:bs": "karma start karma.bs.config.js --coverage",
		"demo": "es-dev-server --app-index demo/index.html --node-resolve --watch --open"
	},
	"publishConfig": {
		"access": "public"
	},
	"@pika/pack": {
		"pipeline": [
			[
				"@pika/plugin-standard-pkg",
				{
					"exclude": [
						"tests/**/*"
					]
				}
			],
			[
				"@pika/plugin-build-node"
			],
			[
				"@pika/plugin-build-web"
			],
			[
				"pika-plugin-minify"
			]
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bmartel/interstice.git"
	},
	"bugs": {
		"url": "https://github.com/bmartel/interstice/issues"
	},
	"devDependencies": {
		"@open-wc/testing": "^2.5.4",
		"@open-wc/testing-karma": "^3.2.42",
		"@open-wc/testing-karma-bs": "^1.3.42",
		"@pika/cli": "^0.2.0",
		"@pika/pack": "^0.5.0",
		"@pika/plugin-build-node": "^0.8.1",
		"@pika/plugin-build-web": "^0.8.1",
		"@pika/plugin-standard-pkg": "^0.8.1",
		"deepmerge": "^4.2.2",
		"lodash": "^4.17.15",
		"pika-plugin-minify": "^0.1.0"
	},
	"dependencies": {
		"@interstice/async": "^0.0.1",
		"@open-wc/lit-helpers": "^0.2.9",
		"lit-element": "^2.2.1",
		"lit-html": "^1.1.2"
	},
	"gitHead": "02cad4f515e07633deea0d2389f557429f169e89"
}
