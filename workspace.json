{
  "version": 2,
  "projects": {
    "demo": {
      "root": "packages/demo",
      "sourceRoot": "packages/demo",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "root": "packages/demo",
            "outputPath": "dist/packages/demo"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "demo:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "demo:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "demo:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/demo"
          ],
          "options": {
            "jestConfig": "packages/demo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/demo/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      },
      "tags": []
    },
    "demo-e2e": {
      "root": "packages/demo-e2e",
      "sourceRoot": "packages/demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/demo-e2e/cypress.json",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/demo-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "demo"
      ]
    },
    "jql": {
      "root": "packages/jql",
      "sourceRoot": "packages/jql/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/jql",
            "tsConfig": "packages/jql/tsconfig.lib.json",
            "project": "packages/jql/package.json",
            "entryFile": "packages/jql/src/index.ts",
            "external": [
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/jql/README.md",
                "input": ".",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/jql/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/jql"
          ],
          "options": {
            "jestConfig": "packages/jql/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "mx": {
      "root": "packages/mx",
      "sourceRoot": "packages/mx/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:rollup",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/mx",
            "tsConfig": "packages/mx/tsconfig.lib.json",
            "project": "packages/mx/package.json",
            "entryFile": "packages/mx/src/index.ts",
            "external": [],
            "assets": [
              {
                "glob": "packages/mx/README.md",
                "input": ".",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/mx/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/mx"
          ],
          "options": {
            "jestConfig": "packages/mx/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
